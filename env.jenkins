# GRAFANA ########################################
# Grafana 服务映射宿主机端口号，可在宿主机127.0.0.1:4000访问
GRAFANA_PORT=4000


# JAEGER #########################################
# Jaeger 服务映射宿主机端口号，可在宿主机127.0.0.1:5100访问
JAEGER_PORT=5100


# DTM #########################################
# DTM HTTP 协议端口号
DTM_HTTP_PORT=36789
# DTM gRPC 协议端口号
DTM_GRPC_PORT=36790


# BEANSTALKD
BEANSTALKD_PORT=11300

REXIX_HOST_PORT=8088

ETL_HOST_PORT=8089

ZOOKEEPER_HOST_PORT=2181
KAFKA_HOST_PORT=9092
KAFKA_DATA_DIR=./data/kafka